function LoadActivePlugins($arg) {
    $plugin = new plugin();
    $plugin->LoadActivePlugins($arg);
}
function PluginLoad($pluginPath, $filePath) {
    $plugin = new plugin();
    $plugin->PluginLoad($pluginPath, $filePath);
}
function PluginURL($path) {
    $plugin = new plugin();
    return $plugin->PluginURL($path);
}